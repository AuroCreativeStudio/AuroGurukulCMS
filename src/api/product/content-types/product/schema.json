{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Product_Id": {
      "type": "uid"
    },
    "Title": {
      "type": "string"
    },
    "Price": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "text"
    },
    "wishlist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wishlist.wishlist",
      "inversedBy": "products"
    }
  }
}
