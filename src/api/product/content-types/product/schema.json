{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Price": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "text"
    },
    "description_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description_video": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "videos"
      ]
    },
    "discount_coupon": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::discount-coupon.discount-coupon",
      "inversedBy": "products"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "all_resource": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::resource.resource",
      "inversedBy": "products"
    },
    "Testimonials_videos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "videos"
      ]
    },
    "short_description": {
      "type": "text"
    },
    "video": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "Download_Catalogue": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Product_Id": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "review"
    },
    "description_points": {
      "type": "component",
      "component": "description-points.description-points",
      "repeatable": true
    },
    "resource_highlights": {
      "type": "component",
      "component": "resource-highlights.resource-highlights",
      "repeatable": true
    }
  }
}
