{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_title": {
      "type": "string",
      "required": true
    },
    "course_hours": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    },
    "why_choose_this_program": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "fees": {
      "type": "string"
    },
    "course_id": {
      "type": "string",
      "required": true
    },
    "assessment": {
      "type": "component",
      "component": "assessment.assessment",
      "repeatable": true
    },
    "teaching_methodology": {
      "type": "component",
      "component": "teaching.teaching-methodology",
      "repeatable": true
    },
    "doubt_clearing_sessions": {
      "type": "component",
      "component": "course.course-overview",
      "repeatable": true
    },
    "support": {
      "type": "component",
      "component": "support.support",
      "repeatable": true
    },
    "duration_component": {
      "type": "component",
      "component": "duration.duration-component",
      "repeatable": true
    },
    "teaching_sessions": {
      "type": "component",
      "component": "sessions.teaching-sessionss",
      "repeatable": true
    },
    "test_assessment": {
      "type": "component",
      "component": "test.test-assessment",
      "repeatable": true
    },
    "study_materials": {
      "type": "component",
      "component": "study.study-materials",
      "repeatable": true
    },
    "all_course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::all-course.all-course",
      "inversedBy": "courses"
    },
    "all_grade": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::all-grade.all-grade",
      "inversedBy": "courses"
    }
  }
}
