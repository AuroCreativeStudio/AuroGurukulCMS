{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_title": {
      "type": "string",
      "required": true
    },
    "course": {
      "type": "string"
    },
    "course_hours": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "fees": {
      "type": "string"
    },
    "why_choose_this_program": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "course_id": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "string"
    },
    "assessment": {
      "type": "component",
      "component": "assessment.assessment",
      "repeatable": true
    },
    "teaching_methodology": {
      "type": "component",
      "component": "teaching.teaching-methodology",
      "repeatable": true
    },
    "course_overview": {
      "type": "component",
      "component": "course.course-overview",
      "repeatable": true
    },
    "support": {
      "type": "component",
      "component": "support.support",
      "repeatable": true
    }
  }
}
