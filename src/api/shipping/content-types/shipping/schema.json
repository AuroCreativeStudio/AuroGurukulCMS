{
  "kind": "collectionType",
  "collectionName": "shippings",
  "info": {
    "singularName": "shipping",
    "pluralName": "shippings",
    "displayName": "Shipping"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Shipping_ID": {
      "type": "string",
      "unique": true,
      "configurable": false
    },
    "Shipping_Method": {
      "type": "enumeration",
      "enum": [
        "Standard",
        "Express",
        "Same-Day",
        "Free Shipping",
        "Pickup"
      ]
    },
    "Order_Status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Processing",
        "Shipped",
        "Out for Delivery",
        "Delivered",
        "Cancelled",
        "Returned",
        "Failed",
        "Refunded"
      ]
    },
    "Shipping_Cost": {
      "type": "biginteger"
    },
    "Zip_Code": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "Product": {
      "type": "component",
      "component": "product-item.product-item",
      "repeatable": true
    },
    "Order_ID": {
      "type": "string"
    },
    "Total_price": {
      "type": "string"
    },
    "Tracking_Number": {
      "type": "string"
    },
    "Courier_Partner": {
      "type": "enumeration",
      "enum": [
        "BlueDart",
        "Delhivery",
        "XpressBees",
        "EcomExpress",
        "Shadowfax"
      ]
    },
    "Estimated_Delivery": {
      "type": "datetime"
    }
  }
}
