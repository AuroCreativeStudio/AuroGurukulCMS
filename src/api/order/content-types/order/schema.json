{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Order_ID": {
      "type": "uid"
    },
    "Price": {
      "type": "string"
    },
    "Date": {
      "type": "date"
    },
    "Payment_Status": {
      "type": "boolean"
    },
    "Coupons": {
      "type": "component",
      "component": "coupons.coupons",
      "repeatable": false
    },
    "Invoice": {
      "type": "component",
      "component": "invoice.invoice",
      "repeatable": false
    },
    "Billing_Address": {
      "type": "component",
      "component": "billing-address.billing-address",
      "repeatable": false
    },
    "Shipping_Address": {
      "type": "component",
      "component": "shipping-address.shipping-address",
      "repeatable": false
    },
    "Product_Item": {
      "type": "component",
      "component": "product-item.product-item",
      "repeatable": true
    },
    "Course_Item": {
      "type": "component",
      "component": "course-item.course-item",
      "repeatable": false
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "PhonePe_Order_Id": {
      "type": "string"
    },
    "Transaction_Id": {
      "type": "string"
    },
    "terms": {
      "type": "boolean"
    },
    "receipt": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "success",
        "failed"
      ]
    },
    "InvoiceNumber": {
      "type": "string"
    },
    "order_type": {
      "type": "enumeration",
      "enum": [
        "checkout",
        "direct_enrollment"
      ]
    }
  }
}
